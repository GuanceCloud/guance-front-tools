{
    "private": true,
    "workspaces": [
        "lib"
    ],
    "config": {
        "path:generated": "./lib/generated"
    },
    "scripts": {
        "build": "npm run clean && npm run generate && npm run build:cjs && npm run build:esm",
        "build:cjs": "tsc -p tsconfig.cjs.json",
        "build:esm": "tsc -p tsconfig.esm.json",
        "clean": "rm -rf ./lib/generated && rm -rf ./lib/esm && rm -rf ./lib/cjs",
        "generate": "node scripts/generate.mjs",
        "format": "prettier -c .",
        "validate": "node scripts/validate.mjs"
    },
    "devDependencies": {
        "ajv": "8.11.0",
        "install": "^0.13.0",
        "json-schema-to-typescript": "^15.0.2",
        "npm": "^10.8.3",
        "prettier": "2.7.1",
        "typescript": ">=4.5"
    },
    "engines": {
        "node": ">=12.7.0"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@grafana/schema": "^11.2.0"
    }
}